{
    "get":
    [
        {
            "route": "/rooms",
            "handlers" : ["getRooms"]
        },
        {
            "route": "/mypanel",
            "handlers" : ["denyNotAuthenticated", "mypanel"]
        },
        {
            "route": "/getUserData",
            "handlers" : ["getUserData"]
        },
        {
            "route": "/rooms/:UID/publicData",
            "handlers": [ "getRoomPublicData" ]
        },
        {
            "route": "/rooms/:UID/ressources/:modCategory/:name",
            "handlers": [ "getRoomResource" ]
        }
    ],
    "post":
    [
        {
            "route": "/createRoom",
            "handlers": ["denyNotAuthenticated", "createRoom"]
        },
        { 
            "route": "/register",
            "handlers": ["register"]
        },
        {
            "route": "/login",
            "handlers": ["login"]
        },
        {
            "route" : "/logout",
            "handlers" : ["denyNotAuthenticated", "logout"]
        }
    ]
}